{
  "name": "web-malain-et-possible",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node --project tsconfig.node.json scripts/sync-agents-json.ts && next dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:bdd": "bddgen test && playwright test",
    "test:bdd:generate": "bddgen test",
    "test:bdd:ui": "bddgen test && playwright test --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:generate": "tsx scripts/generate-e2e-scenario.ts",
    "build": "ts-node scripts/increment-site-version.ts sync && ts-node scripts/increment-site-version.ts patch && ts-node scripts/increment-site-version.ts build",
    "build:with-tests": "npm test && npm run test:e2e && ts-node scripts/increment-site-version.ts sync && ts-node scripts/increment-site-version.ts patch && ts-node scripts/increment-site-version.ts build",
    "build:analyze": "ANALYZE=true ts-node scripts/increment-site-version.ts sync && ts-node scripts/increment-site-version.ts patch && next build",
    "start": "next start",
    "lint": "eslint",
    "metrics:collect": "ts-node scripts/collect-metrics-simple.ts",
    "metrics:view": "npm run dev",
    "metrics:full": "npm run metrics:collect && npm run dev",
    "publie": "ts-node scripts/publie.ts",
    "precommit": "ts-node scripts/pre-commit-generate-e2e.ts",
    "version:patch": "ts-node scripts/increment-site-version.ts patch",
    "version:minor": "ts-node scripts/increment-site-version.ts minor",
    "version:sync": "ts-node scripts/increment-site-version.ts sync",
    "extract:pdf": "tsx scripts/extract-pdf-to-markdown.ts",
    "analyse:bilan": "tsx scripts/analyse-bilan-competences.ts",
    "sync:agents": "ts-node --project tsconfig.node.json scripts/sync-agents-json.ts",
    "snapshot:local": "tsx scripts/snapshot-page.ts http://localhost:3000 .cursor/snapshot-local.txt",
    "snapshot:prod": "tsx scripts/snapshot-page.ts https://web-malain-et-possible.vercel.app/ .cursor/snapshot-production.txt",
    "screenshot:local": "tsx scripts/screenshot-page.ts http://localhost:3000 .cursor",
    "screenshot:prod": "tsx scripts/screenshot-page.ts https://web-malain-et-possible.vercel.app/ .cursor",
    "screenshot:compare": "tsx scripts/screenshot-compare.ts",
    "hierarchie:generate": "ts-node --project tsconfig.node.json scripts/generate-hierarchie-site.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "next": "^16.1.6",
    "playwright-bdd": "^8.4.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "swagger-ui-react": "^5.31.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.4",
    "@playwright/mcp": "^0.0.62",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "cloc": "^2.6.0-cloc",
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "eslint-plugin-complexity": "^1.0.2",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "lighthouse": "^13.0.1",
    "lint-staged": "^16.2.7",
    "pdf-parse": "^1.1.4",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "type-coverage": "^2.29.7",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
